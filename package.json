{
  "name": "cerebral",
  "version": "0.0.0-semantically-released",
  "description": "A state controller with its own debugger",
  "main": "src/index.js",
  "scripts": {
    "pretest": "standard",
    "test": "istanbul cover nodeunit -- tests",
    "test:watch": "nodeunit-watcher tests",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebral/cerebral.git"
  },
  "devDependencies": {
    "commitizen": "^2.5.0",
    "conventional-changelog": "0.0.17",
    "cz-customizable": "^2.7.0",
    "ghooks": "^1.0.3",
    "istanbul": "^0.4.4",
    "nodeunit": "^0.9.0",
    "nodeunit-watcher": "0.0.3",
    "semantic-release": "^4.3.5",
    "standard": "^7.1.2",
    "validate-commit-msg": "^2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "analyzeCommits": "./sr-prerelease.js"
  },
  "dependencies": {
    "cerebral-model": "^0.1.1",
    "cerebral-model-immutable": "^0.1.3",
    "cerebral-operators": "^0.2.1",
    "lodash": "^4.6.1"
  }
}

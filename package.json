{
  "name": "cerebral-monorepo",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:demos": "node ./scripts/run.js prepublish demos",
    "checkdeps": "node ./scripts/check.dependencies.js --auto-fix",
    "commit": "git-cz",
    "commitmsg": "node ./node_modules/cz-customizable-ghooks/lib/index.js .git/COMMIT_EDITMSG",
    "coverage": "node ./scripts/run.js coverage",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "deploy": "cd packages/website/publisher && npm run ci",
    "format": "npm run lint -- --fix && prettier --write --with-node-modules \"**/*.md\" \"**/*.css\" \"**/*.json\"",
    "link": "node ./scripts/run.js link",
    "lint": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "postcoverage": "istanbul-combine -d coverage -p summary -r lcov -r html packages/**/coverage/*.json",
    "postinstall": "npm run setup",
    "postsetup": "npm run link",
    "precommit": "lint-staged",
    "predeploy": "node ./scripts/run.js prepublish demos",
    "pretest": "npm run checkdeps",
    "release": "babel-node ./scripts/release/index.js",
    "setup": "node ./scripts/run.js prepublish nodemos",
    "test": "node ./scripts/run.js test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "dependencies": {
    "addressbar": "1.0.3",
    "aphrodite": "1.2.0",
    "baobab": "2.4.3",
    "bulma": "0.2.3",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "classnames": "2.2.5",
    "es6-error": "4.0.2",
    "eventemitter3": "2.0.2",
    "faye-websocket": "0.11.1",
    "feature.js": "1.0.0",
    "firebase": "4.8.0",
    "firebase-admin": "4.0.4",
    "firebase-queue": "1.6.0",
    "js-joda": "1.1.13",
    "match-media": "0.2.0",
    "md5": "^2.2.1",
    "mobx": "3.4.1",
    "mobx-state-tree": "1.0.0",
    "offline-js": "0.7.18",
    "prism": "4.1.2",
    "proxyquire": "1.7.11",
    "raf": "3.3.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "shortway": "0.3.0",
    "todomvc-app-css": "2.1.0",
    "todomvc-common": "1.0.4",
    "ua-parser-js": "0.7.10",
    "universal-websocket-client": "1.0.1",
    "url-mapper": "1.1.1",
    "uuid": "3.1.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write --with-node-modules",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "devDependencies": {
    "@angular/common": "4.4.0-RC.0",
    "@angular/compiler": "4.4.0-RC.0",
    "@angular/core": "4.3.6",
    "@angular/platform-browser": "4.4.0-RC.0",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@types/assert": "0.0.31",
    "@types/classnames": "^2.2.3",
    "@types/express": "4.11.1",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.7",
    "@types/prop-types": "^15.5.2",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/uuid": "^3.4.3",
    "assert": "1.4.1",
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^22.0.6",
    "babel-plugin-inferno": "^4.0.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-version-transform": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-preact": "1.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "babel-register": "^6.23.0",
    "babel-watch": "2.0.5",
    "commitizen": "^2.9.6",
    "concurrently": "3.4.0",
    "core-js": "2.5.0",
    "coveralls": "^2.12.0",
    "cross-env": "^5.0.0",
    "cz-customizable": "^4.0.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.11.0",
    "execa": "^0.6.3",
    "express": "4.15.4",
    "firebase-server": "0.7.1",
    "fs-extra": "2.0.0",
    "gh-pages": "1.0.0",
    "husky": "^0.13.2",
    "inferno": "^4.0.3",
    "inferno-create-element": "^4.0.3",
    "inferno-shared": "^4.0.4",
    "istanbul-combine": "^0.3.0",
    "jest": "22.4.3",
    "jsdom": "^11.5.1",
    "lint-staged": "^7.0.0",
    "marksy": "4.0.0",
    "mocha": "^4.1.0",
    "mock-socket": "6.0.4",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "nyc": "^10.3.2",
    "preact": "8.1.0",
    "prettier": "^1.11.1",
    "prismjs": "1.6.0",
    "prop-types": "15.6.1",
    "react-scripts": "1.1.1",
    "react-scripts-ts": "2.14.0",
    "react-test-renderer": "^16.2.0",
    "shx": "0.2.2",
    "string-template": "1.0.0",
    "symlink-dir": "^1.0.2",
    "ts-node": "5.0.1",
    "typescript": "2.7.2",
    "typescript-definition-tester": "0.0.5",
    "typescript-eslint-parser": "^14.0.0",
    "vue": "2.3.3",
    "xhr-mock": "2.1.0",
    "yargs": "6.6.0",
    "zone.js": "0.8.17",
    "parcel-bundler": "1.7.0"
  }
}

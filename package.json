{
  "name": "cerebral-monorepo",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "checkdeps": "node ./scripts/check.dependencies.js --auto-fix",
    "commit": "git-cz",
    "commitmsg": "node ./node_modules/cz-customizable-ghooks/lib/index.js .git/COMMIT_EDITMSG",
    "coverage": "node ./scripts/run.js coverage",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "deploy": "cd packages/website/publisher && npm run ci",
    "format": "npm run lint -- --fix && prettier --write --with-node-modules \"**/*.md\" \"**/*.css\" \"**/*.json\"",
    "link": "node ./scripts/run.js link",
    "lint": "eslint \"**/*.js\" \"**/*.ts\"",
    "postcoverage": "istanbul-combine -d coverage -p summary -r lcov -r html packages/**/coverage/*.json",
    "postinstall": "npm run setup",
    "postsetup": "npm run link && node ./scripts/run.js setup all",
    "precommit": "lint-staged",
    "predeploy": "node ./scripts/run.js prepublish",
    "pretest": "npm run checkdeps",
    "release": "babel-node ./scripts/release/index.js",
    "setup": "node ./scripts/run.js prepublish nodemos",
    "test": "node ./scripts/run.js prepublish nodemos && node ./scripts/run.js test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "dependencies": {
    "bulma": "0.2.3",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "es6-error": "4.0.2",
    "eventemitter3": "2.0.2",
    "faye-websocket": "0.11.1",
    "local-storage": "^2.0.0",
    "md5": "^2.2.1",
    "page": "1.8.6",
    "prism": "4.1.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "universal-websocket-client": "1.0.1",
    "uuid": "3.1.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write --with-node-modules",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "devDependencies": {
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@types/mocha": "5.1.0",
    "@types/page": "1.5.32",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.3.11",
    "@types/react-dom": "16.0.5",
    "@types/uuid": "3.4.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "assert": "1.4.1",
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^22.0.6",
    "babel-plugin-inferno": "^4.0.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-version-transform": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-preact": "1.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "babel-register": "^6.23.0",
    "babel-watch": "2.0.5",
    "commitizen": "^2.9.6",
    "concurrently": "3.4.0",
    "core-js": "2.5.0",
    "coveralls": "^2.12.0",
    "cross-env": "^5.0.0",
    "cz-customizable": "^4.0.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^0.6.3",
    "express": "4.15.4",
    "fs-extra": "2.0.0",
    "gh-pages": "1.0.0",
    "husky": "^0.13.4",
    "inferno": "^4.0.3",
    "inferno-create-element": "^4.0.3",
    "inferno-shared": "^4.0.4",
    "istanbul-combine": "^0.3.0",
    "jest": "22.4.3",
    "jsdom": "^11.5.1",
    "lint-staged": "^7.0.0",
    "marksy": "4.0.0",
    "mocha": "^4.1.0",
    "mock-socket": "6.0.4",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "nyc": "^10.3.2",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-typescript": "^1.0.0",
    "preact": "8.1.0",
    "prettier": "^1.14.0",
    "prismjs": "1.6.0",
    "prop-types": "15.6.1",
    "raf": "^3.3.0",
    "react-scripts": "1.1.1",
    "react-scripts-ts": "2.14.0",
    "react-test-renderer": "^16.2.0",
    "shx": "0.2.2",
    "string-template": "1.0.0",
    "symlink-dir": "^1.0.2",
    "ts-jest": "^22.4.3",
    "ts-node": "6.0.5",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.6.3",
    "typescript-definition-tester": "0.0.5",
    "typescript-eslint-parser": "^14.0.0",
    "vue": "2.3.3",
    "xhr-mock": "2.1.0",
    "yargs": "6.6.0",
    "zone.js": "0.8.17"
  }
}

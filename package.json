{
  "name": "cerebral-monorepo",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:demos": "node ./scripts/run.js prepublish demos",
    "checkdeps": "node ./scripts/dependencies.test.js",
    "commit": "git-cz",
    "commitmsg": "node ./node_modules/cz-customizable-ghooks/lib/index.js .git/COMMIT_EDITMSG",
    "coverage": "node ./scripts/run.js coverage",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "deploy": "cd website/publisher && npm run ci",
    "link": "node ./scripts/run.js link",
    "lint": "standard",
    "postcoverage": "istanbul-combine -d coverage -p summary -r lcov -r html packages/**/coverage/*.json babel/**/coverage/*.json",
    "postinstall": "npm run setup",
    "postsetup": "npm run link",
    "prelint": "npm run checkdeps",
    "pretest": "npm run lint",
    "setup": "node ./scripts/run.js prepublish nodemos",
    "test": "node ./scripts/run.js test"
  },
  "dependencies": {
    "addressbar": "1.0.3",
    "aphrodite": "1.2.0",
    "bulma": "0.2.3",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "eventemitter3": "2.0.2",
    "faye-websocket": "0.11.1",
    "feature.js": "1.0.0",
    "firebase": "3.9.0",
    "firebase-admin": "4.0.4",
    "firebase-queue": "1.6.0",
    "js-joda": "1.1.13",
    "match-media": "0.2.0",
    "offline-js": "0.7.18",
    "proxyquire": "1.7.11",
    "raf": "3.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "shortway": "0.3.0",
    "todomvc-app-css": "2.0.6",
    "todomvc-common": "1.0.2",
    "ua-parser-js": "0.7.10",
    "universal-websocket-client": "1.0.1",
    "url-mapper": "1.1.1",
    "uuid": "2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "noDefaultIgnore": true,
    "ignore": [
      "/node_modules/**",
      "/packages/*/**/node_modules/**",
      "**/build/",
      "**/*.min.js",
      "**/bundle.js",
      "coverage/**",
      "vendor/**"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-version-transform": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.23.0",
    "commitizen": "^2.9.6",
    "concurrently": "3.4.0",
    "coveralls": "^2.12.0",
    "cross-env": "^5.0.0",
    "cz-customizable": "^4.0.0",
    "cz-customizable-ghooks": "^1.5.0",
    "execa": "^0.6.3",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.0",
    "firebase-server": "0.7.1",
    "gh-pages": "1.0.0",
    "husky": "^0.13.2",
    "istanbul-combine": "^0.3.0",
    "jest": "^18.1.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.4.1",
    "mocha-jsdom": "^1.1.0",
    "mock-socket": "6.0.4",
    "node-sass": "4.5.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "prop-types": "15.5.10",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "^1.0.6",
    "shx": "^0.2.2",
    "standard": "github:gaspard/standard#v8.6.0fix2",
    "symlink-dir": "^1.0.2",
    "typescript": "2.3.2",
    "typescript-definition-tester": "0.0.5",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "xhr-mock": "1.9.0"
  }
}

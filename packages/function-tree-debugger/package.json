{
  "name": "cerebral-debugger",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "start": "DEBUGGER_TARGET=electron NODE_ENV=development concurrently -k 'node ./electron/devServer.js' 'npm run electron'",
    "electron": "electron .",
    "build:chromeExtension": "NODE_ENV=production webpack --config webpack.chromeExtension.config",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config && npm run prepare:electron && npm run package:electron",
    "prepare:electron": "rimraf electron/dist electron/node_modules && node scripts/createElectronSymlink",
    "package:electron": "npm run package:electron:mac && npm run package:electron:win && npm run package:electron:linux",
    "package:electron:mac": "electron-packager ./electron CerebralDebugger --platform=darwin --out ./electron/dist",
    "package:electron:win": "electron-packager ./electron CerebralDebugger --platform=win32 --out ./electron/dist",
    "package:electron:linux": "electron-packager ./electron CerebralDebugger --platform=linux --out ./electron/dist"
  },
  "repository": "https://github.com/cerebral/cerebral",
  "author": "Christian Alfoni",
  "dependencies": {
    "cerebral": "2.0.0-0-alpha.d81c2c44",
    "cerebral-module-useragent": "1.0.0-0-alpha.d81c2c44",
    "classnames": "^2.2.5",
    "color": "^0.11.4",
    "electron-default-menu": "^1.0.0",
    "electron-json-storage": "^2.1.0",
    "prismjs": "^1.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "electron": "^1.4.1",
    "electron-packager": "^8.3.0",
    "express": "^4.14.0",
    "react-scripts": "0.7.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}

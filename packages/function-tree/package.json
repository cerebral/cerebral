{
  "name": "function-tree",
  "version": "0.5.0-b",
  "description": "When a function is not enough",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register 'src/**/*.test.js'",
    "build": "BABEL_ENV=production babel src/ --out-dir=lib/ -s",
    "copy-declarations": "node copy-declarations.js",
    "coverage": "nyc --reporter=lcov --reporter=json npm run test",
    "prepublish": "npm run build && npm run copy-declarations"
  },
  "devDependencies": {
    "@cerebral/monorepo": "0.0.1",
    "@types/eventemitter3": "^1.2.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "eventemitter3": "^2.0.2",
    "universal-websocket-client": "^1.0.1"
  },
  "typings": "lib/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "keywords": [
    "javascript",
    "function",
    "flow",
    "complex",
    "state"
  ],
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "contributors": [
    "Aleksey Guryanov <gurianov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebral/cerebral/issues"
  },
  "homepage": "https://github.com/cerebral/cerebral/tree/master/packages/function-tree#readme",
  "nyc": {
    "exclude": [
      "node_modules",
      "lib",
      "tests",
      "**/*.test.js",
      "**/testHelper.js"
    ]
  }
}

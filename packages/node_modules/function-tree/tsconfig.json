{
  "compilerOptions": {
    "outDir": "./lib/fluent",
    "target": "es5",
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [
      "es2015.promise",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "dom",
      "es5",
      "es2015.collection",
      "scriptHost"
    ]
  },
  "include": ["src/fluent/*.ts"],
  "exclude": ["**/*.test.ts", "node_modules"]
}

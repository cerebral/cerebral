    "proxyquire": "^1.7.11"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "keywords": [
    "javascript",
    "firebase",
    "queue",
    "function-tree"
  ],
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "contributors": [
    "Aleksey Guryanov <gurianov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebral/cerebral/issues"
  },
  "homepage": "https://github.com/cerebral/cerebral/tree/master/packages/firebase-admin#readme",
  "nyc": {
    "exclude": [
      "node_modules",
      "lib",
      "tests",
      "**/*.test.js",
      "**/testHelper.js"
    ]
  },
  "devDependencies": {
    "firebase-server": "^0.7.1"
  }

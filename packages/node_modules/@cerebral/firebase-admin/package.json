{
  "name": "@cerebral/firebase-admin",
  "version": "1.0.0-b",
  "description": "Firebase admin execution and provider",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src/ --out-dir=lib/ -s",
